@model Member_Organisations.ViewModels.IndexViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_BootstrapLayout.cshtml";
}

<div text-align: center;">
    <h1>Homepage</h1>
    <h4>Made with bootstrap!</h4>
    <p>
        Content will be here.
    </p>

    <div>
        <ul>
            @foreach (var item in Model.members)
            {
                <li><strong>Name:</strong> @item.FName @item.LName <strong>| Address:</strong> @item.Address <strong>| Phone Number:</strong> @item.PhoneNum</li>
                <br />
            }
        </ul>
        <ul>
            @foreach (var item in Model.orgs)
            {
                <li>@item.Name</li>
                <br />
            }
        </ul>
    </div>
</div>



