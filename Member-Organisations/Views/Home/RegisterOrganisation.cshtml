@model Member_Organisations.ViewModels.RegisterOrganisationViewModel
@{
    Layout = "~/Views/Shared/_RegisterLayout.cshtml";
    Page.Title = "Register Organisation";
}


    

<div class="container" style="text-align: center;">
    
    <h1>Register Organisation</h1>

    <form name="registerForm" method="post" style="width: 320px; margin: auto; padding: 30px 0 60px 0;">
        @Html.LabelFor(m => m.org.Name, "Organisation Name: ")
        @Html.EditorFor(m => m.org.Name, new { htmlAttributes = new { @class = "form-control" } })<br>
        <button type="submit" class="btn btn-success">Registrera</button>
    </form>
</div>
